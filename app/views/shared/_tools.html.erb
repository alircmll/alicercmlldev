<section class="timeline" id="tab-react">
        <div class="container-about text-center">
          <h1>Outils</h1>
        </div>
      <ul>
        <li>
          <div class="timeline-contentt">
            <time><h3>Collaboration</h3></time>
            <%= image_tag('Git.png', class:"img img-timeline") %>

            <%= image_tag('github.svg', class:"img img-timeline") %>

            <%= image_tag('slack.png', class:"img img-timeline") %>
          </div>
        </li>
        <li>
          <div class="timeline-contentt">
            <time><h3>Conception</h3></time>
            <%= image_tag('whim.png', class:"img img-timeline") %>

            <%= image_tag('Figma.svg', class:"img img-timeline") %>

            <%= image_tag('database.png', class:"img img-timeline") %>
          </div>
        </li>
        <li>
          <div class="timeline-contentt">
            <time><h3>Développement</h3></time>
              <%= image_tag('ruby.png', class:"img img-timeline") %>

              <img class="img img-timeline" src="https://seeklogo.com/images/R/ruby-on-rails-logo-95951CC5FB-seeklogo.com.png" alt="rails">

              <%= image_tag('js.png', class:"img img-timeline" ) %>

              <%= image_tag('html5.svg', class:"img img-timeline") %>

              <%= image_tag('css-3.svg', class:"img img-timeline") %>

              <%= image_tag('jquer.png', class:"img img-timeline" ) %>

              <%= image_tag('Boots.png', class:"img img-timeline" ) %>

              <%= image_tag('Postgresql.svg', class:"img img-timeline") %>

              <%= image_tag('mailchimp.png', class:"img img-timeline") %>

              <%= image_tag('stripe.png', class:"img img-timeline stripe") %>
          </div>
        </li>
        <li>
          <div class="timeline-contentt">
            <time><h3>Production</h3></time>
              <%= image_tag('heroku.png', class:"img img-timeline") %>
          </div>
        </li>
      </ul>
    </section>

    <section class="et-slide" id="tab-flexbox" id= "section-profil" id="counter-stats" class="wow fadeInRight" data-wow-duration="1.4s">
        <div class="container">
          <div class="row container-profil">
            <div class="col-lg-3 stats">
              <i class="fa fa-code" aria-hidden="true"></i>
              <div class="counting" data-count="40000">0</div>
              <h5>Lignes de code</h5>
            </div>

            <div class="col-lg-3 stats">
              <i class="fa fa-check" aria-hidden="true"></i>
              <div class="counting" data-count="3">0</div>
              <h5>Projets</h5>
            </div>

            <div class="col-lg-3 stats">
              <i class="fa fa-coffee" aria-hidden="true"></i>
              <div class="counting" data-count="999">0</div>
              <h5>Tasses de café</h5>
            </div>
          </div>
        </div>
    </section>
